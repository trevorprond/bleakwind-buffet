<UserControl x:Class="PointOfSale.EntreeCustomization.Thugs_T_Bone"
      xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
      xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
      xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
      xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
      xmlns:local="clr-namespace:PointOfSale.EntreeCustomization"
      mc:Ignorable="d" 
      d:DesignHeight="650" d:DesignWidth="450"
      >

    <Grid>
        <DockPanel Height="600" VerticalAlignment="Bottom">
            <TextBox DockPanel.Dock ="Top" Text="Customize Thugs T-Bone" Height="50"/>
            <Button Grid.Row="2"
                Content="Done" Margin="9" ClickMode="Release" Click="BacktoBeginning"/>
        </DockPanel>
    </Grid>
</UserControl>
